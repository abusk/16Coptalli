{
  "name": "cc-ui",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "typings install",
    "clean": "rm -rf _dist",
    "builddev": "npm run tsc && npm run clean && mkdir _dist && webpack --devtool inline-source-map ",
    "build": "npm run tsc && npm run clean && mkdir _dist && webpack -p",
    "start": "npm run tsc && npm run clean && mkdir _dist && webpack-dev-server --progress",
    "minBuild": "webpack && uglifyjs ./_dist/bundle.js -c -m -o ./_dist/bundle.min.js --source-map ./_dist/bundle.min.js.map",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular2-datatable": "^0.5.2",
    "angular2-highlight-js": "^4.0.0",
    "angular2-tree-component": "2.0.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.5.0",
    "core-js": "^2.4.1",
    "lodash": "^4.17.2",
    "ng2-bootstrap": "^1.2.1",
    "ng2-bs3-modal": "^0.10.4",
    "ng2-charts": "^1.5.0",
    "ng2-file-upload": "^1.1.2",
    "ng2-sweetalert2": "^0.0.8",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.0.0",
    "css-loader": "^0.26.0",
    "html-loader": "^0.4.4",
    "lite-server": "^2.2.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "typings": "^1.4.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.4.4"
